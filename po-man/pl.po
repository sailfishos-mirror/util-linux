msgstr ""

#. type: Labeled list
#: ../sys-utils/prlimit.1.adoc:78
#, no-wrap
msgid "*-l*, *--memlock*[**=**_limits_]"
msgstr ""

#. type: Plain text
#: ../sys-utils/prlimit.1.adoc:80
